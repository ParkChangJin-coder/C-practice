#include <stdio.h>

int main(){
  while(1){
    int input = 0;
    printf("input number : ");
    scanf("%d", &input);
    printf("1~10사이에서 %d를 입력");
    if(!(input >= 1 && input <= 10)){
      printf("input between 1 to 10\n");
      continue;
      //반복문 제일 위로 올라간다
      
    }
  }
    
  
  
  return 0;
}