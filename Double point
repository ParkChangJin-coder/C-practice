#include <stdio.h>

int main(){
  
  int num1 = 10;
  
  int* pnum1 = &num1;
  printf("pnum adress : %d\n", pnum1);
  printf("pnum adress value : %d\n", *pnum1);
  int** ppnum1 = &pnum1;
  printf("ppnum1 adress : %d\n", ppnum1);
  printf("*ppnum1 adress value : %d\n", *ppnum1);
  printf("**ppnum1 adress value : %d\n", **ppnum1);
  return 0;
}