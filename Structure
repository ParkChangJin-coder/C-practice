#include <stdio.h>

struct Point{
  int x;
  int y;
};

struct student{
  int number;
  char name[50];
  float mathScore;
};

int main(){
  
  struct Point p1;
  
  struct Point p2 ={10, 12};
  
  struct student s1 = {1, "kim", 84};
  printf("p2 x : %d\n", p2.x);
  printf("p2 y : %d\n", p2.y);
  
  printf("student number : %d\n", s1.number);
  printf("student name : %s\n", s1.name);
  printf("student mathScore : %f\n", s1.mathScore);
  
  return 0;
}